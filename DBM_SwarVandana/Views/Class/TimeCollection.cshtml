@model List<Models.ClassTimingPatterns>
@using Code;
@{
    Layout = null;
}

@if (Model.Count > 0)
{
    <table cellpadding="0" cellspacing="0" class="table">
        <tr bgcolor="#ccc">
            <td class="td">Week Day</td>
            <td class="td">Start Time</td>
            <td class="td">End Time</td>
        </tr>
        @foreach (var v in Model)
        {
            <tr>
                <td class="td">@(((WeekDays)Enum.Parse(typeof(WeekDays), v.WeekDayId.ToString())).Desciption()) </td>
                <td class="td">@v.StartTime</td>
                <td class="td">@v.EndTime</td>
            </tr>
        }
    </table>
}
